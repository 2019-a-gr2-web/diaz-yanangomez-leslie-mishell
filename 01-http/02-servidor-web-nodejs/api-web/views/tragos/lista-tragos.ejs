<!doctype html>
<html lang="es">
<head>
    <meta charset="UTF-8">
    <meta name="viewport"
          content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>Document</title>
    <style>
        table,tr,td{
            border: black 1px solid;
        }
    </style>
</head>
<body>
<a href="crear"><button>Crear</button></a>
<table>
    <tr>
        <th>Nombre</th>
        <th>Grados</th>
        <th>Caduca</th>
        <th>Precio</th>
        <th>Tipo</th>
        <th>Acciones</th>
    </tr>
    <% arregloTragos1.forEach((trago)=> { %>
    <tr>
        <td><%=trago.nombre%></td>
        <td><%=trago.gradosAlcohol%></td>
        <td><%=trago.fechaCaducidad%></td>
        <td><%=trago.precio%></td>
        <td><%=trago.tipo%></td>
        <td>
            <button>Editar</button>
            <form action="eliminar" method="POST">
                <label for="id"></label>
                <input type="number" id="id" name="id" hidden value="<%=trago.id%>">
                <button>Eliminar</button>
            </form>
        </td>
        <%})%>
    </tr>
</table>
</body>
</html>